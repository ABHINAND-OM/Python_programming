class rectangle:
    def _init_(self):
        self.length=int(input("enter the length :"))
        self.breadth=int(input("enter the breadth :"))
    def area(self):
        self.a=self.length*self.breadth
        print("area of the rectangle:",self.a)
        return self.a
    def perimeter(self):
        self.p=2*(self.length+self.breadth)
        print("perimeter of the rectangle :",self.p)
        return self.p
x=rectangle()
print("\n rectangle 1")
x._init_()
a1=x.area()
x.perimeter()

y=rectangle()
print("\n rectangle 2")
y._init_()
b1=y.area()
y.perimeter()
if(a1>b1):
    print("rectangle 1 is big")
elif(a1==b1):
    print("area of the rectangles are same")
else:
    print("rectangle 2 is big")
